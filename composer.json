{
    "name": "gigcodes/laravel-country-info",
    "description": "Adds country, language and timezone data and ORM models.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gigcodes\\CountryInfo\\": "src/",
            "Gigcodes\\CountryInfo\\Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "authors": [
        {
            "name": "Karthick",
            "email": "karthick@gigcodes.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gigcodes\\CountryInfo\\Providers\\CountryInfoServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
